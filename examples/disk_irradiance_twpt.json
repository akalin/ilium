{
  "scene": {
    "aggregate": {
      "type": "PrimitiveList",
      "primitives": [
        {
          "_include": "disk_irradiance_scene.json"
        }
      ]
    }
  },

  "renderer": {
    "type": "TwoWayPathTracingRenderer",
    "pathTypes": [
      "emittedLight",
      "directLighting",
      "emittedImportance",
      "directSensor"
    ],
    "weighingMethod": "uniform",
    "russianRouletteMethod": "fixed",
    "russianRouletteStartIndex": 2,
    "russianRouletteMaxProbability": 1,
    "maxEdgeCount": 2,
    "sampler": {
      "type": "IndependentSampler"
    }
  }
}
